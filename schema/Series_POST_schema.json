{
  "required": [
    "ParentUID",
    "ReferentType",
    "SeasonClass",
    "SequenceNumber",
    "Title"
  ],
  "type": "object",
  "properties": {
    "SeasonUID": {
      "pattern": "^(.*)$",
      "type": "string"
    },
    "EpisodeUID": {
      "pattern": "^(.*)$",
      "type": "string"
    },
    "ParentUID": {
      "minLength": 1,
      "pattern": "^(.*)$",
      "type": "string"
    },
    "StructuralType": {
      "pattern": "^(.*)$",
      "type": "string",
      "enum": [
        "Abstraction",
        "Performance",
        "Digital"
      ]
    },
    "ReferentType": {
      "pattern": "^(.*)$",
      "type": "string",
      "enum": [
        "Series",
        "Season",
        "TV",
        "Movie",
        "Short",
        "Web",
        "Compilation",
        "Supplemental"
      ]
    },
    "SequenceNumber": {
      "type": "integer",
      "format": "int32"
    },
    "Title": {
      "minLength": 1,
      "pattern": "^(.*)$",
      "type": "string"
    },
    "TitleClass": {
      "minLength": 1,
      "pattern": "^(.*)$",
      "type": "string"
    },
    "SeasonClass": {
      "pattern": "^(.*)$",
      "type": "string",
      "enum": [
        "Adjunct",
        "Main",
        "Miniseries",
        "Pro Forma",
        "Recut"
      ]
    },
    "EntityType": {
      "pattern": "^(.*)$",
      "type": "string"
    },
    "EidrId": {
      "pattern": "^(.*)$",
      "type": "string"
    },
    "CreateDateTime": {
      "pattern": "^(.*)$",
      "type": "string"
    },
    "UpdateDateTime": {
      "pattern": "^(.*)$",
      "type": "string"
    },
    "PublishedStatus": {
      "pattern": "^(.*)$",
      "type": "string"
    },
    "ReleaseDate": {
      "pattern": "^(.*)$",
      "type": "string"
    },
    "ContentIDList": {
      "type": "array",
      "items": {
        "required": [
          "ContentID"
        ],
        "type": "object",
        "properties": {
          "ContentID": {
            "pattern": "^(.*)$",
            "type": "string"
          }
        }
      }
    }
  }
}